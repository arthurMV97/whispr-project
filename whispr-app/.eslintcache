ect to=\"/\"/>}\r\n          </Route>\r\n\r\n          <Route path=\"/mysecureadmin\">\r\n            {!isAdmin && !isUserLogged ? <HomeAdmin /> : <Redirect to=\"admin-dashboard\"/>}\r\n          </Route>\r\n          <Route path=\"/admin-dashboard\">\r\n            {isAdmin ? <AdminDashboard /> : <Redirect to=\"/\"/>}\r\n          </Route>\r\n\r\n        </Switch>\r\n       \r\n    </Router>\r\n  )\r\n}\r\n\r\nexport default App;\r\n",["156","157"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\reportWebVitals.js",[],["158","159"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Header.jsx",[],["160","161"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Home.jsx",["162"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Decouvrir.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Connexion.jsx",[],["163","164"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Profil.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Inscription.jsx",["165"],"import React, { Component } from 'react';\r\nimport axios from 'axios';\r\nimport {Link} from 'react-router-dom'\r\n\r\n\r\n\r\nclass Inscription extends Component {\r\n    constructor() {\r\n        super();\r\n        this.state = {\r\n            nom: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            prenom: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            date:{\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            email: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            password: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            confirmation: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n            msg: {\r\n                content: '',\r\n                msg: '',\r\n                valid: null\r\n            },\r\n        }\r\n    }\r\n\r\n   \r\n    handleChange = event => {\r\n        const targetName = event.target.name\r\n        const obj = this.handleValidation(event)\r\n        if (obj.bool) {\r\n            this.setState(prevState => ({\r\n                [targetName]: {                   \r\n                    ...prevState[targetName],    \r\n                    content: event.target.value,\r\n                    valid: obj.bool,\r\n                    msg: obj.msg      \r\n                }\r\n            }))\r\n        } else {\r\n            this.setState(prevState => ({\r\n                [targetName]: {                   \r\n                    ...prevState[targetName],    \r\n                    msg: obj.msg,\r\n                    valid: obj.bool     \r\n                }\r\n            }))\r\n        }\r\n        console.log(this.state.date);\r\n    }\r\n\r\n    handleValidation = ev => {\r\n        const response = {\r\n            msg: '',\r\n            bool: false\r\n        }\r\n        const val = ev.target.value\r\n        const name = ev.target.name\r\n        // const rgxSpace = /(\\s)/g\r\n        const rgxNames = /^[a-z ,.'-]+$/i\r\n        const rgxEmails = /^[\\w-\\.]+@([\\w-]+\\.)+[\\w-]{2,4}$/g\r\n        const rgxPass = /^(?=.*[A-Za-z])(?=.*\\d)[A-Za-z\\d]{8,}$/\r\n\r\n        //Champ vide\r\n        if (val === '') {\r\n            response.msg = `Champs obligatoire pour ${name}` \r\n            return response\r\n        }\r\n\r\n        //Nom Prénom\r\n        else if ((name === 'nom' || name === 'prenom') && !rgxNames.test(val)) {\r\n            response.msg = `Veuillez entrer un ${name} valide`\r\n            return response\r\n        }\r\n\r\n        //Emails\r\n        else if ((name === 'email') && !rgxEmails.test(val)) {\r\n            response.msg = `Veuillez entrer un ${name} valide`\r\n            return response\r\n        }\r\n\r\n        //Mots de passe \r\n        else if ((name === 'password' || name === 'confirmation') && !rgxPass.test(val)) {\r\n            response.msg = `Votre mot de passe doit contenir 8 caractères minimum, 1 lettre et 1 chiffre`\r\n            return response\r\n        }\r\n\r\n        else {\r\n            response.bool = true\r\n            response.msg = 'Champ valide'\r\n            return response\r\n        }\r\n    }\r\n\r\n    formValidation() {\r\n        for (const element in this.state) {\r\n           if(this.state[element].valid === false) {\r\n               return false\r\n           } \r\n    }\r\n       return true\r\n    }\r\n    handleSubmit =  event => {\r\n        event.preventDefault();\r\n        const isFormValid = this.formValidation()\r\n        \r\n       if (isFormValid) {\r\n        if (this.state.confirmation === this.state.password) {\r\n            const user = {\r\n                nom: this.state.nom.content,\r\n                prenom: this.state.prenom.content,\r\n                date: this.state.date.content,\r\n                email: this.state.email.content,\r\n                password: this.state.password.content,\r\n                        }\r\n\r\n            axios.post('http://localhost:8080/sign-up', user)\r\n                .then(res => {\r\n                    console.log(res.data)\r\n                })\r\n        } else {\r\n            this.setState(prevState => ({\r\n                confirmation: {                   \r\n                    ...prevState.confirmation,    \r\n                    msg: 'Votre mot de passe ne correspond pas',\r\n                        \r\n                }\r\n            }))\r\n        }\r\n       } else {\r\n           window.alert(\"Les champs du formulaire ne sont pas valables\")\r\n       }\r\n        \r\n\r\n    }\r\n\r\n    render() {\r\n        return (\r\n            <div >\r\n             <h1>Inscription</h1>\r\n             <div className=\"connexion-forms\">\r\n            <form onSubmit={this.handleSubmit} >\r\n                <input type=\"text\" name=\"nom\" id=\"nom\" placeholder=\"Nom\" onChange={this.handleChange} required />\r\n                <p className={this.state.nom.valid ? 'valid-input' : 'not-valid-input'}>{this.state.nom.msg}</p>\r\n                <input type=\"text\" name=\"prenom\" id=\"prenom\" placeholder=\"Prénom\" onChange={this.handleChange} required />\r\n                <p className={this.state.prenom.valid ? 'valid-input' : 'not-valid-input'}>{this.state.prenom.msg}</p>\r\n                <input type=\"date\" name=\"date\" id=\"date\" className=\"date\" onChange={this.handleChange} required />\r\n                <p className={this.state.date.valid ? 'valid-input' : 'not-valid-input'}>{this.state.date.msg}</p>\r\n                <input type=\"email\" name=\"email\" id=\"email\" placeholder=\"Email*\" onChange={this.handleChange} required />\r\n                <p className={this.state.email.valid ? 'valid-input' : 'not-valid-input'}>{this.state.email.msg}</p>\r\n                <input type=\"password\" name=\"password\" id=\"password\" placeholder=\"Mot de passe\" onChange={this.handleChange} required />\r\n                <p className={this.state.password.valid ? 'valid-input' : 'not-valid-input'}>{this.state.password.msg}</p>\r\n                <input type=\"password\" name=\"confirmation\" id=\"confirm\" placeholder=\"Confirmez votre mot de passe\" onChange={this.handleChange} required />\r\n                <p className={this.state.confirmation.valid ? 'valid-input' : 'not-valid-input'}>{this.state.confirmation.msg}</p>\r\n\r\n                <button type=\"submit\" className=\"full-btn\">Inscription</button>\r\n            </form>\r\n            <div>\r\n                <p>Vous êtes déjà inscris ? <Link  to=\"/connexion\">Connectez-vous ici.</Link></p>\r\n            </div>\r\n            </div>\r\n            </div>\r\n        );\r\n    }\r\n}\r\n\r\nexport default Inscription;","C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\index.js",[],["166","167"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\store\\reducer\\user.js",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\store\\reducer\\allReducers.js",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Dashboard.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\store\\actions\\user.js",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Admin.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\HomeAdmin.jsx",[],["168","169"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\AdminDashboard.jsx",["170"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\store\\reducer\\admin.js",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\SinglePoste.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\store\\actions\\admin.js",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\ProfileResume.jsx",["171"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\MesFavoris.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\MesPostes.jsx",["172"],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\Separation.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\PopUpAbonnements.jsx",[],"C:\\Users\\morin\\Desktop\\SIMPLON\\whispr-project\\whispr-app\\src\\components\\SingleUser.jsx",[],"C:\\Users\\morin\\Deskt